{
  "title": "Cmd+tab maps to Ctrl+tab",
  "rules": [
    {
      "description": "CMD+TAB to CTRL+TAB",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "control"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "CTRL+TAB to CMD+TAB ",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "control"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "command"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "CMD+SHIFT+TAB to CTRL+SHIFT+TAB",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "command",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "control",
                "shift"
              ]
            }
          ]
        }
      ]
    },
    {
      "description": "CTRL+SHIFT+TAB to CMD+SHIFT+TAB",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "tab",
            "modifiers": {
              "mandatory": [
                "control",
                "shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "tab",
              "modifiers": [
                "command",
                "shift"
              ]
            }
          ]
        }
      ]
    }
  ]
}